static SYSERRORCODE_E GetDevInfo ( char* msgBuf ); //获取设备信息



typedef SYSERRORCODE_E ( *cmd_fun ) ( char* msgBuf ); //获取SN的回调

typedef struct
{
	const char* cmd_id;            /* 命令id */
	cmd_fun  fun_ptr;     /* 函数指针 */
} CMD_HANDLE_T;

CMD_HANDLE_T CmdList[] =
{
	{"201",  OpenDoor},
	{"1006", AbnormalAlarm},
	{"1012", AddCardNo},
	{"1013", DelCardNo},
	{"1016", UpgradeDev},
	{"1017", UpgradeAck},
	{"1021", EnableDev},
	{"1023", SetDevParam},
	{"1024", SetJudgeMode},
	{"1026", GetDevInfo}
};


SYSERRORCODE_E exec_proc ( char* cmd_id, char* msg_buf )
{

	SYSERRORCODE_E result = NO_ERR;
	int i = 0;

	for ( i = 0; i < DIM ( CmdList ); i++ )
	{
		if ( 0 == strcmp ( CmdList[i].cmd_id, cmd_id ) )
		{
			CmdList[i].fun_ptr ( msg_buf );
			return ;
		}
	}
	printf ( "invalid id %s\n", cmd_id );



	return result;
}


  
